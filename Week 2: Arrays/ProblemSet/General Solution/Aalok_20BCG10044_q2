prices = input()
prices = prices.strip('[')
prices = prices.strip(']')
prices = prices.split(',')
prices = list(map(int,prices))

first = prices[0]
for i in range(1,len(prices)):
    if prices[i]>first:
        highest = max(prices[i:])
        print(highest-first)
        break 
    else:
        first = prices[i]
        if i==len(prices)-1:
            print('0')
            break
        



